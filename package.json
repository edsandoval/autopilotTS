{
  "name": "autopilotts",
  "version": "1.0.0",
  "description": "AI-Powered Ticket Resolution System for Terminal",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "autopilot": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && copyfiles -u 1 \"src/web/public/**/*\" dist",
