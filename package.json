    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "fast-glob": "^3.3.3",
    "figlet": "^1.7.0",
    "inquirer": "^8.2.5",
    "open": "^11.0.0",
    "simple-git": "^3.22.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
